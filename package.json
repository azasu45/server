    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "puppeteer": "^19.6.3",
    "redux-typescript": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sharp": "^0.31.3",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
